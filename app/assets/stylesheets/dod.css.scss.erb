body {
	font-family: Helvetica, Arial, Verdana, Sans-serif;
	font-size: 12px;
	line-height: 1.4;
	color: #FFF;
	background: #222 url(<%= image_path('dod/page_bg.png') %>) no-repeat top center;
	margin: 0;
	&.home {
		font-family: Times New Roma, Times, serif;
		background-color: #111;
		background-image: url(<%= image_path('dod/dark-bg.jpg') %>);
		background-repeat: repeat;
		text-align: center;
		div#footer {
			position: absolute;
			bottom: 20px;
			font-family: Times New Roma, Times, serif;
			width: 100%;
			text-align: center;
			color: #333;
			a {
				color: #666;
				text-decoration: none;
				&:hover {
					color: #999;
				}
			}
			div#footer_info {
				position: static;
				text-align: center;
			}
			div#logo {
				float: none;
				text-align: center;
			}
		}
		div#middle_container {
			text-align: center;
			margin: 0;
			padding: 0;
			position: absolute;
			left: 0;
			top: 20%;
			width: 100%;
		}
		h1 {
			letter-spacing: normal;
		}
	}
}
