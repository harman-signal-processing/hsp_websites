/**** Application Layout *****/
div#locale_container {
	margin-top: 30px;
	img {
		height: 400px;
	}
	a {
		color: white;
		font-size: 110%;
	}
}
#locale_switcher {
	position: absolute;
	top: 0px;
	left: 0px;
	padding: 2px 3px 0px;
  background: url(<%= image_path('dark-gray-80.png') %>);
	z-index: 999;
