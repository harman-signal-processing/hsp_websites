.support_box_container {
	margin: 0px 150px;
	h2 {
		margin-left: 20px;
	}
	form.contact {
		margin-left: 20px;
	}
	.support_box {
		background: #fff;
		border: 1px solid #ECECEC;
		width: 280px;
		height: 120px;
		overflow: hidden;
		float: left;
		padding: 8px;
		margin: 10px;
		h4 {
			font-size: 14px;
			line-height: 20px;
			margin-bottom: 6px;
			color: #444;
