	font-size: 16px;
}
.product_detail_box h2 a:link,
.product_detail_box h2 a:visited {
	color: #727063;
	background: #eeebda;
	background: -moz-linear-gradient(top,  #eeebda 0%, #e9e6d1 40%, #e1ddc3 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#eeebda), color-stop(40%,#e9e6d1), color-stop(100%,#e1ddc3));
	background: -webkit-linear-gradient(top,  #eeebda 0%,#e9e6d1 40%,#e1ddc3 100%);
	background: -o-linear-gradient(top,  #eeebda 0%,#e9e6d1 40%,#e1ddc3 100%);
	background: -ms-linear-gradient(top,  #eeebda 0%,#e9e6d1 40%,#e1ddc3 100%);
	background: linear-gradient(top,  #eeebda 0%,#e9e6d1 40%,#e1ddc3 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#eeebda', endColorstr='#e1ddc3',GradientType=0 );
	display: block;
	-moz-border-radius: 3px;
	border-radius: 3px;
	border: 1px solid #e9e6d1;
	padding-left: 12px;
	padding-top: 2px;
}
.product_detail_box h2 a:hover {
