html {
    height: 100%;
}
body {
	font-family: Helvetica, Arial, Verdana, Sans-serif;
	font-size: 12px;
	line-height: 1.4;
	color: #242321;
	background: #fefced url(<%= image_path('vocalist/bg.png') %>);
	height: 100%;
	margin: 0; padding: 0;
	background-attachment: fixed;
}
@font-face {
    font-family: 'ChunkFiveRegular';
    src: font-url('Chunkfive-webfont.eot');
    src: font-url('Chunkfive-webfont.eot?#iefix') format('embedded-opentype'),
         font-url('Chunkfive-webfont.woff') format('woff'),
         font-url('Chunkfive-webfont.ttf') format('truetype'),
         font-url('Chunkfive-webfont.svg#ChunkFiveRegular') format('svg');
    font-weight: normal;
    font-style: normal;

}
p {
	margin-bottom: 8px;
}
a:link, a:visited {
	color: #800000;
}
li {
	margin-bottom: 6px;
}
h1, h2 {
	margin-bottom: 8px;
	padding: 0px;
	color: #A59164;
/*	letter-spacing: -0.06em;*/
}
h1.page_title {
	text-align: left;
	padding: 6px 20px 2px;
	background: url(<%= image_path('vocalist/title_bg.png') %>);
	color: #FFF;
	font-size: 26px;
	line-height: 32px;
	width: 920px;
	letter-spacing: 0.1em;
	margin-left: -10px;
	margin-top: -8px;
	font-weight: normal;
}
h1{
	font-family: 'ChunkFiveRegular', Helvetica, Arial, Verdana, Sans-serif;
	font-size: 32px;
	line-height: 40px;
}
h2{
	font-size: 20px;
	line-height: 36px;
}
h2 a:link,
h2 a:visited {
	text-decoration: none;
	color: #A59164;
}
#big_bottom_box_container h3 a:link,
#big_bottom_box_container h3 a:visited {
	text-decoration: underline;
}
h3 {
	font-size: 18px;
	line-height: 20px;
	margin-bottom: 4px;
}
h4{
	font-size: 16px;
}
h5{
	font-size: 14px;
}
h6{
	font-size: 12px;
}
.tiny {
	font-size: 10px;
}
.clear {
	clear: both;
}
#preload {
	display: none;
}

/**** Application Layout *****/
div#locale_container {
	margin-top: 30px;
	img {
		height: 400px;
	}
	a {
		font-size: 110%;
	}
}
#locale_switcher {
	position: absolute;
	top: 0px;
	left: 0px;
	padding: 2px 3px 0px;
  background: url(<%= image_path('white-90.png') %>);
	z-index: 999;
	a {
		text-decoration: none;
	}
	#locale_selections {
		margin-top: 6px;
		padding-bottom: 4px;
		display: none;
		a {
			margin: 0 -2px 0px -8px;
			display: block;
			padding: 4px 8px;
			&:hover {
				background: #CCC;
			}
		}
	}
}
div#Outer {
	width: 960px;
	background: #fefced;
	box-shadow: #B4AA92 0px 2px 14px;
	margin: 0px auto 0px;
	padding-top: 10px;
}
div#Header {
	width: 960px;
	margin-left: auto;
	margin-right: auto;
/*	margin-top: 16px;*/
	position: relative;
	z-index: 5000;
}
div#topnav {
	clear: both;
	border-top: 4px double #dfd2b3;
	border-bottom: 4px double #dfd2b3;
	height: 23px;
	margin-top: 3px;
	padding-top: 8px;
	padding-left: 20px;
	font-size: 11px;
	font-weight: bold;
	text-transform: uppercase;
	/*letter-spacing: -0.09em;*/
}
div#topnav ul {
}
div#topnav li {
	display: inline;
	list-style: none;
/*	padding: 1px;*/
/*	margin-top: 4px;*/
}
div#topnav li a:link,
div#topnav li a:visited {
/*	border-right: 1px solid #a2b6ca;*/
/*	text-shadow: #999 1px 1px 1px;*/
