/******* Promo page *******/
.promotion_bug_container {
	width: 860px;
	margin-left: 50px;
	margin-bottom: 30px;
}
.promotion_bug {
	float: left;
	margin-right: 20px;
}
/******** Twitter feed *******/
#twitter_feed_container {
	width: 260px;
    height: 380px;
    background: #222;
	padding: 2px;
}
#twitter_feed_container h2 {
	margin-left: 6px;
}
#twitter_feed_container h2 img {
	vertical-align: middle;
}
#twitter_feed_container #tweets {
	height: 300px;
	overflow: auto;
	background: #000;
	color: #FFF;
}
#twitter_feed_container #tweets .tweet {
	padding: 5px;
	border-bottom: 1px dotted #666;
	font-size: 12px;
	line-height: 14px;
}
#twitter_feed_container .tweet a:link,
#twitter_feed_container .tweet a:visited {
	color: #69cbff;
}
#twitter_feed_container .tweet .tw_actions {
	font-size: 80%;
	margin: 0; padding: 0;
	margin-top: 4px;
}
#twitter_feed_container #social_media_links {
	margin-top: 8px;
}
#right_column #product_packaging_contents ul li {
	list-style-type: square;
	margin-left: 14px;
}
/************** ***********/
.disclaimer {
	font-size: 80%; 
	margin-bottom: 10px;
	font-style:oblique; 
	color: #333
}
/*** admin functions **/
.admin_function {
	position: absolute;
	top: 4px;
	right: 10px;
	color: white;
	z-index: 100;
}
.admin_function a:link,
.admin_function a:visited {
	color: white;
	text-decoration: none;
	display: block;
	background: #5A89CC;
	padding: 1px 6px 3px 6px;
	border-radius: 4px;
	-moz-border-radius: 6px;
}
.admin_function a:hover {
	color: white;
	text-decoration: none;
	background: #0080FF;
}
#admin_menu_link {
	position: absolute;
	top: 0;
	right: 0;
	z-index: 1000;
}
#admin_menu_link a:link,
#admin_menu_link a:visited {
	display: block;
	color: #EEE;
	text-decoration: none;
	padding: 1px 6px 3px 6px;
	font-family: Helvetica, arial, sans-serif;
	font-weight: normal;
	font-size: 10px;
	text-shadow: #222 1px 1px 1px;
	background: #00395a;
}
#admin_menu_link a:hover {
	color: #FFF;
}
.small_button {
	font-size: 11px; 
	font-weight: normal; 
	border-radius: 3px; 
	border: 1px solid #CCC; 
	padding: 1px 5px;
}
.callout {
	background: #EEE;
	box-shadow: 1px 2px 3px #BBB;
	padding: 8px 12px;
	margin: 8px 0px;
}
a {
	.software_banner {
		border: 1px solid #c1c1c1;
		box-shadow: 1px 2px 4px #CCC;
		background: #fbfbfb;
		&:hover {
			background: #eaeaea;
		}
		h3 {
			text-decoration: none;
		}
	}
}
div.quicklinks {
	margin: 20px 0px;
	ul {
		list-style-type: disc;
		margin: 10px 20px 30px;
		li {
			font-size: 13px;
			margin-left: 20px;
		}
	}
}