div#feature_container {
	position: relative;
	width: 960px;
	text-align: center;
	margin-left: auto;
	margin-right: auto;
	height: 392px;
	z-index: 300;
	background: none;
}
div.slideshow_frame {
	position: absolute;
	left: 0px;
	z-index: 200;
}
div#slideshow_controls {
	position: absolute;
	bottom: 10px;
	right: 20px;
	z-index: 1000;
}
div#slideshow_controls a {
	color: #F00;
	text-decoration: none;
	display: block;
	background: #000;
	font-weight: bold;
	margin-right: 6px;
	padding: 3px 8px 2px 8px;
	-moz-border-radius: 4px;
	border-radius: 4px;
	float: right;
}
div#slideshow_controls a:hover {
	color: #000;
	background: #333;
}
div#slideshow_controls a.current_button {
