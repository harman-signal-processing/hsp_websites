.related_products {
	margin: 20px 0px;
	font-style: oblique;
}
table.comparison {
	border-collapse: collapse;
	border: 1px solid #CCC;
	td {
		padding: 5px;
		border: 1px dotted #CCC;
		&.spec_name {
			width: 120px;
			background: #EEE
		}
	}
	th {
		padding: 4px;
		border: 1px solid #CCC;
		border-bottom: 2px solid #999;
	}
}
div.compare_button {
	padding: 20px 10px;
}
/***** Product Page *****/
div#product_content {
	margin-bottom: 10px;
	position: relative;
	.suggested_product a {
		text-decoration: none;
		color: #555;
		h3 {
			color: #000;
		}
	}
	#right_column {
		position: absolute;
		top: -10px;
		right: 14px;
		z-index: 100;
		text-align: left;
		width: 222px;
		padding: 3px 6px;
		h4 {
			text-align: center;
			font-size: 10px;
			line-height: 14px;
			font-weight: 100;
		}
	}
	#left_column {
		float: left;
		text-align: center;
		width: 940px;
		position: relative;
		#product_basics {
			color: #797979;
			font-size: 14px;
			line-height: 20px;
			h1 {
				font-family: 'ImpactRegular', sans-serif;
				font-size: 34px;
				line-height: 36px;
				color: #1A1A1A;
				text-shadow: #A5A5A5 0px 1px 1px;
			}
			h3 {
				color: #797979;
				margin-bottom: 16px;
				font-weight: normal;
			}
		}
		h2.price {
			font-family: 'ImpactRegular', sans-serif;
			font-size: 20px;
			line-height: 26px;
			color: #AE9A6A;
		}
		#viewer_container {
			height: 250px;
			width: 940px;
			background: white;
			text-align: center;
			display: table;
			margin-bottom: 40px;
			tr td {
				width: 940px;
				height: 250px;
				text-align: center;
				vertical-align: middle;
			}
		}
	}
	.larger_tag {
		text-align: left;
		margin-left: 16px;
		font-size: 90%;
		color: #777;
	}
}
ul#product_thumbnails {
	margin: 8px 0px 8px 16px;
	padding: 0px;
	text-align: left;
	width: 670px;
