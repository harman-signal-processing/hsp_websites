}
div#middle_container {
	text-align: center;
	width: 1000px;
	margin-left: auto;
	margin-right: auto;
	margin-top: -14px;
	padding-left: 5px;
}
div.product_family_feature {
	background: #FFF;
	border: 1px solid #CCC;
	margin: 20px;
	padding: 10px;
	width: 250px;
	height: 275px;
	float: left;
	text-align: left;
	-moz-border-radius: 10px;
	border-radius: 10px;
	color: #000;
	h3 {
		border-top: 1px dotted #666;
		padding-top: 4px;
		margin-top: 6px;
	}
}
.homepage_feature {
	display: inline-block;
	width: 210px;
	height: 125px;
	overflow: hidden;
	margin-left: 13px;
	margin-right: 24px;
}
div.placeholder {
	border: 5px solid #CCC;
	background: #EEE;
	color: #666;
	margin-bottom: 5px;
	margin-left: auto; 
	margin-right: auto;
}
div.family_photo {
	height: 125px; width: 150px; 
}
.family_button {
	margin-right: 17px;
}
div#signup {
