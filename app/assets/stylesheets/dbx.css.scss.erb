html {
    height: 100%;
}
body {
	font-family: Helvetica, Arial, Verdana, Sans-serif;
	font-size: 12px;
	line-height: 1.4;
	color: #FFF;
	background: #4c4c4c url(<%= image_path('dbx/page_bg.gif') %>) repeat-x;
	background-position: middle;
	height: 100%;
	letter-spacing: 0.02em;
	margin: 0;
}
@font-face {
    font-family: 'ImpactRegular';
    src: url('/fonts/impact-webfont.eot');
    src: url('/fonts/impact-webfont.eot?#iefix') format('embedded-opentype'),
         url('/fonts/impact-webfont.woff') format('woff'),
         url('/fonts/impact-webfont.ttf') format('truetype'),
         url('/fonts/impact-webfont.svg#ImpactRegular') format('svg');
    font-weight: normal;
    font-style: normal;
}
p {
	margin-bottom: 8px;
}
a:link, a:visited {
	color: #F00;
}
li {
	margin-bottom: 6px;
}
h1, h2 {
	margin-bottom: 8px;
	padding: 0px;
	color: #FFF;
	font-family: 'ImpactRegular', Helvetica, Arial, sans-serif;
	font-weight: 200;
}
.internal_page h1.page_title,
h1.page_title {
	text-align: left;
	background: black;
	color: red;
	padding: 8px 16px 6px;
	font-size: 17px;
	line-height: 21px;
	letter-spacing: 0.1em;
	font-weight: 200;
	text-shadow: 0px 1px 0px #2B1F1F;
	text-transform: uppercase;
	margin-top: -10px;
	margin-left: -10px;
	width: 929px;
	border-top-left-radius: 6px;
	border-top-right-radius: 6px;
}
.floating_message {
	position: absolute;
	background: #F0F0F0;
	border: 1px solid #CCC;
	box-shadow: 0px 0px 8px #222;
	font-size: 140%;
	border-radius: 6px;
	-moz-border-radius: 6px;
	width: 500px;
	top: -16px;
	left: 190px;
	z-index: 10000;
	padding: 10px 36px;
	
}
h1{
	font-size: 32px;
	line-height: 40px;
}
h2{
	font-size: 20px;
	line-height: 36px;
/*	font-family: Helvetica, Arial, sans-serif;*/
}
h2 a:link,
h2 a:visited {
	text-decoration: none;
	color: #222;
}
#big_bottom_box_container h2 a:link,
#big_bottom_box_container h2 a:visited {
	text-decoration: underline;
}
h3{
	font-size: 18px;
	line-height: 20px;
	margin-bottom: 4px;
	font-family: Helvetica, Arial, sans-serif;
}
h4{
	font-size: 16px;
}
h5{
	font-size: 14px;
}
h6{
	font-size: 12px;
}
.tiny {
	font-size: 10px;
	font-weight: normal;
}
.hint {
	font-size: 90%;
	color: #333;
	font-style: oblique;
}
.clear {
	clear: both;
}
#preload {
	display: none;
}

/**** Application Layout *****/
div#Header {
	width: 960px;
	margin: 16px auto 16px auto;
	position: relative;
	z-index: 5000;
	div#search_form {
		position: absolute;
		top: 48px;
		right: 10px;
		width: 130px;
		input#query {
			background: #601012;
			color: #FFF;
			border: 1px solid #800000;
			width: 110px;
			padding-left: 4px;
			padding-right: 16px;
			font-size: 12px;
			-moz-border-radius: 4px;
			border-radius: 4px;
		}
	}
}
div#market_segment_nav {
	position: absolute;
	right: 0px;
	bottom: 39px;
	a:link, a:visited {
		padding: 4px;
		margin-left: 8px;
		font-style: oblique;
		color: #EEE;
		text-shadow: 0px 1px 0px #000;
		text-decoration: none;
		font-size: 140%;
	}
	a:hover {
		color: #F00;
		text-decoration: none;
	}
}
div#tagline {
	position: absolute;
	top: 18px;
	left: 183px;
}
div#topnav {
	float: right;
/*	border: 1px solid #cf0404;*/
	background: #be1300;
	background: -moz-linear-gradient(top,  #be1300 0%, #8d0303 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#be1300), color-stop(100%,#8d0303));
	background: -webkit-linear-gradient(top,  #be1300 0%,#8d0303 100%);
	background: -o-linear-gradient(top,  #be1300 0%,#8d0303 100%);
	background: -ms-linear-gradient(top,  #be1300 0%,#8d0303 100%);
	background: linear-gradient(top,  #be1300 0%,#8d0303 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#be1300', endColorstr='#8d0303',GradientType=0 );
	height: 21px;
	width: 780px;
	margin-top: 44px;
	padding-top: 6px;
	padding-left: 2px;
	font-size: 11px;
/*	font-weight: bold;*/
	box-shadow: 0px 2px 4px #000;
	text-transform: uppercase;
	/*letter-spacing: -0.09em;*/
	-moz-border-radius: 4px;
	border-radius: 4px;
	li {
		display: inline;
		list-style: none;
		a:link, a:visited {
			border-right: 1px solid #C33437;
			text-shadow: 1px 1px 1px #444;
			padding: 2px 7px 3px 7px;
			color: #EAEAEA;
			text-decoration: none;
			margin-top: 2px;
		}
		a:hover {
			color: #FFF;
		}
	}
}

div#content_container {
	width: 960px;
	margin-left: auto;
	margin-right: auto;
}
div#content_container_content {
	padding: 10px;
}
.internal_page {
	background: #FEFFFF;
	box-shadow: 0px 4px 7px #111;
	color: black;
	-moz-border-radius: 6px;
	border-radius: 6px;
	min-height: 350px;
	h1, h2 {
		color: #222;
	}
}
div#logo {
	float: left;
	text-align: right;
	width: 150px;
	h2 {
		font-size: 30px;
		color: white;
		margin-top: -5px;
		text-align: center;
		width: 170px;
		a:link, a:visited, a:hover {
			color: white;
			text-decoration: none;
			text-shadow: #000 1px 1px 1px;
		}
	}
}
div#big_bottom_box_container {
	background-color: #2e2e2e;
	color: white;
	margin: 20px auto 5px auto;
	width: 960px;
	border: 1px solid #313131;
	-moz-border-radius: 10px;
	border-radius: 6px;
	padding-bottom: 6px;
	h2 {
		font-size: 16px;
		text-shadow: 0px 1px 1px #000;
	}
}
div#big_bottom_box_content {
	padding: 10px;
	padding-bottom: 20px;
	a:link, a:visited {
		color: #ccc;
	}
	h2 {
		a:link, a:visited {
			color: white;
		}
	}
	.small_product_box {
		a:link, a:visited {
			color: #F00;
		}
	}
}

.divider_above {
	border-top: 1px dotted #7283AF; 
	margin-top: 20px;
}
div#footer {
	width: 960px;
	margin-left: auto;
	margin-right: auto;
	font-size: 10px;
	font-family: Helvetica, Arial, Sans-serif;
	color: #fff;
	margin-bottom: 30px;
	text-align: center;
	height: 90px;
	a:link, a:visited {
		color: white;
	}
	div#footer_info {
		float: right;
		text-align: right;
		margin-top: 10px;
		margin-right: 10px;
	}
	div#footer_left {
		float: left;
		margin-left: 10px;
		img {
			margin-bottom: -3px;
			margin-right: 2px;
		}
	}
}
#social_media_links {
	text-align: center;
	margin-bottom: 10px;
	img {
		margin-left: 8px;
		margin-right: 8px;
	}
}
.medium_centered_box {
	width: 500px;
	margin-left: auto;
	margin-right: auto;
	margin-top: 20px;
	margin-bottom: 30px;
	padding: 20px;
}
.wide_centered_box {
	width: 790px;
	margin-left: auto;
	margin-right: auto;
	margin-top: 4px;
	margin-bottom: 10px;
}
.article_centered_box {
	margin: 10px 10%;
	h1 {
		font-size: 24px;
		line-height: 27px;
		color: #444;
	}
	h2 {
		color: #898989;
	}
	.little {
		font-style: oblique;
		color: #555;
	}
}
.light_gray {
	background: #EEE;
}
/***** columns ******/
div.two_column_container {
	text-align: center;
}
div.two_column_container .column_one {
	float: left;
	padding: 10px;
	width: 45%;
	text-align: left;
}
div.two_column_container .column_two {
	float: right;
	padding: 10px;
	width: 45%;
	text-align: left;
}
