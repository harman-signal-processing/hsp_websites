div.product_main_tab_content {
	padding-right: 20px;
}
ul#product_main_tabs {
	margin: 0px -10px 8px -10px;
	padding: 0;
	padding-left: 2px;
	border-bottom: 1px solid #E6E6E6;
	height: 47px;
	background: url(<%= image_path('lexicon/light_gray_gradient_reversed.png') %>) repeat-x bottom;
	li {
		float: left;
		margin: 21px 6px 0px;
		a:link, a:visited {
			display: block;
			padding: 4px 10px;
			border: 1px solid #E6E6E6;
			border-bottom: none;
			color: #666;
			text-decoration: none;
			text-transform: uppercase;
			background: white;
		}
		a:hover {
			color: #111;
		}
		&.current {
			margin-top: 17px;
			a {
				padding-bottom: 7px;
				padding-top: 6px;
				border-bottom: none;
				color: #111;
			}
		}
	}
}

#description {
	text-align: left;
	margin-top: 10px;
	margin-bottom: 10px;
}
#breadcrumbs {
	font-size: 90%;
	text-align: left;
	margin: 0px;
	margin-bottom: 4px;
	letter-spacing: 0.02em;
}
.product_detail_box {
	margin-bottom: 10px;
	h2 {
		margin: 0; padding: 0;
		font-size: 16px;
		a:link, a:visited {
			color: #777;
			border-bottom: 1px solid #E6E6E6;
			background: #FFF url(<%= image_path('lexicon/browse_products_arrow.png') %>) no-repeat;
			background-position: 4px 9px;
			display: block;
			padding-left: 19px;
			padding-top: 2px;
			line-height: 28px;
		}
	}
}

#product_packaging_tabs {
	ul {
		margin:0; padding:0;
		margin-bottom: -8px;
	}
	li {
		margin:0; padding:0;
		display: inline;
		list-style: none;
		border: none;
		a {
			display: block;
			float: left;
			height: 30px;
			padding: 3px 4px 2px 2px;
