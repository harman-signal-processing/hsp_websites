body {
	font: {
		family: Helvetica, "Lucida Sans", Arial, Verdana, Sans-serif;
		size: 13px;
	}
	line-height: 1.4;
	color: #FFF;
	background: #000f40 url(<%= image_path('digitech/homepageBG.png') %>) repeat-x;
	letter-spacing: 0.02em;
}
@font-face {
	font-family: 'Scriber';
	src: url('/fonts/scriber_bold-webfont.eot?iefix=') format('embedded-opentype'),
		url('/fonts/scriber_bold-webfont.woff') format('woff'), 
		url('/fonts/scriber_bold-webfont.ttf') format('truetype'), 
		url('/fonts/scriber_bold-webfont.svg#webfontvCLvcxrc') format('svg');
	font-weight: normal;
	font-style: normal;
}
p {
	margin-bottom: 8px;
}
a:link, a:visited {
	color: blue;
}
li {
	margin-bottom: 6px;
}

h1 {
	font-size: 32px;
	line-height: 40px;
	margin-bottom: 8px;
	padding: 0px;
	color: #FFF;
	letter-spacing: -0.06em;
	&.page_title {
		text-align: center;
	}
}
h2 {
	font-size: 20px;
	line-height: 36px;
	margin-bottom: 8px;
	padding: 0px;
	color: #FFF;
	letter-spacing: -0.06em;
	a:link, a:visited {
		text-decoration: none;
		color: #222;
	}
}
h3 {
	font-size: 18px;
	line-height: 20px;
	margin-bottom: 4px;
}
h4 {
	font-size: 16px;
}
h5 {
	font-size: 14px;
}
h6 {
	font-size: 12px;
}
.tiny {
	font-size: 10px;
}
.clear {
	clear: both;
}
#preload {
	display: none;
}

/**** Application Layout *****/
header#main_header {
	width: 960px;
	margin: 16px auto 0px;
	position: relative;
	z-index: 5000;
}
nav#topnav {
	float: right;
	border: 1px solid #a2b6ca;
	background: {
		image: url(<%= image_path('digitech/topmenuBG.png') %>);
		repeat: repeat-x;
		position: center;
	}
	height: 23px;
	width: 770px;
	margin-top: 3px;
	padding-top: 6px;
	padding-left: 2px;
	font-size: 12px;
	text-transform: uppercase;
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px;
	li {
		display: inline;
		list-style: none;
		a:link, a:visited {
			border-right: 1px solid #a2b6ca;
			padding: 2px 7px 3px 7px;
			color: white;
			text-decoration: none;
			margin-top: 2px;
			font-family: 'Scriber';
		}
		a:hover {
			text-decoration: underline;
		}
	}
}

header#main_header div#search_form {
	position: absolute;
	top: 9px;
	right: 10px;
	width: 130px;
	input#query {
		background: #14277c url(<%= image_path('digitech/searchMagGlass.png') %>) no-repeat right;
		color: white;
		border: 1px solid #000f40;
		width: 110px;
		padding-left: 4px;
		padding-right: 16px;
		font-size: 12px;
		-webkit-border-radius: 8px;
		-moz-border-radius: 8px;
		border-radius: 8px;
	}
}

div#content_container {
	width: 960px;
	margin-left: auto;
	margin-right: auto;
	-webkit-box-shadow: 0px 2px 20px #00061D;
    -moz-box-shadow: 0px 2px 20px #00061D;
	box-shadow: 0px 2px 20px #00061D;
}
div#content_container_content {
	padding: 10px;
}
.internal_page {
	background: #FEFFFF;
	color: black;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	min-height: 350px;
	h1, h2 {
		color: #222;
	}
}
div#logo {
	float: left;
	margin-top: 5px;
	margin-left: 10px;
}
div#big_bottom_box_container {
	background-color: #00092a;
	color: white;
	margin: 20px auto 5px auto;
	width: 960px;
	border: 1px solid #192955;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	padding-bottom: 10px;
	-webkit-box-shadow: 0px 2px 20px #00061D;
	-moz-box-shadow: 0px 2px 20px #00061D;
	box-shadow: 0px 2px 20px #00061D;
	h2 {
		font-size: 16px;
	}
}
div#big_bottom_box_content {
	padding: 10px;
	padding-bottom: 20px;
	a:link, :visited {
		color: #ccc;
	}
	h2 {
		a:link, a:visited {
			color: white
		}
	}
}

.divider_above {
	border-top: 1px dotted #7283AF; 
	margin-top: 20px;
}
footer {
	width: 940px;
	margin: 10px auto;
	font-size: 10px;
	font-family: Arial, Helvetica, Sans-serif;
	color: #fff;
	text-align: center;
	height: 60px;
	a:link, a:visited {
		color: white
	}
	nav#footer_info {
		float: right;
		text-align: right;
		margin-top: 10px;
	}
	div#footer_left {
		float: left;
		img {
			margin-bottom: -3px;
			margin-right: 2px;
		}
	}
}


#social_media_links {
	text-align: center;
	margin-bottom: 10px;
	img {
		margin-left: 8px;
		margin-right: 8px;
	}
}
.medium_centered_box {
	width: 500px;
	margin: 20px auto 30px;
	padding: 20px;
}
.wide_centered_box {
	width: 790px;
	margin: 4px auto 10px;
}
.article_centered_box {
	margin: 10px 8% 25px;
	h1 {
		font-size: 24px;
		line-height: 27px;		
	}
	.little {
		font-style: oblique;
		color: #555;		
	}
}

.light_gray {
	background: #EEE;
}
/***** columns ******/
div.two_column_container {
	text-align: center;
	.column_one {
		float: left;
		padding: 10px;
		width: 45%;
		text-align: left;
	}
	.column_two {
		float: right;
		padding: 10px;
		width: 45%;
		text-align: left;
	}
}
/**** Main ******/
#feature_spacer {
	position: relative;
	height: 380px;
}
#feature_bg {
	position: absolute;
	top: 0px;
	z-index: -100;
	width: 100%;
	height: 445px;
	text-align: center;
	background: {
		image: url(<%= image_path('digitech/BlankFeature_BG.png') %>);
		repeat: no-repeat;
		position: top center;
	}
}
div#feature_container {
	position: relative;
	width: 960px;
	text-align: center;
	margin: 10px auto 0px;
	height: 360px;
	z-index: 300;
}
div.slideshow_frame {
	position: absolute;
	top: -73px;
	left: 0px;
	z-index: 200;
}
div#slideshow_controls {
	position: absolute;
	bottom: 10px;
	right: -6px;
	z-index: 1000;
	a {
		color: #314481;
		text-decoration: none;
		display: block;
		border: 1px solid #314481;
		background: #00092a;
		font-weight: bold;
		margin-right: 6px;
		padding: 2px 8px;
		-webkit-border-radius: 6px;
		-moz-border-radius: 6px;
		border-radius: 6px;
		float: right;
		&:hover {
			border-color: #a2b6ca;
		}
		&.current_button {
			color: #00092a;
			background: #314481;			
		}
	}
}

div#middle_container {
	text-align: center;
	width: 990px;
	margin-left: auto;
	margin-right: auto;
	padding-left: 11px;
}
div.product_family_feature {
	background: #FFF;
	border: 1px solid #CCC;
	margin: 20px;
	padding: 10px;
	width: 250px;
	height: 275px;
	float: left;
	text-align: left;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	color: #000;
	h3 {
		border-top: 1px dotted #666;
		padding-top: 4px;
		margin-top: 6px;
	}
}
.homepage_feature {
	width: 225px;
	height: 125px;
	overflow: hidden;
	margin-left: 2px;
	margin-right: 13px;
}
div.placeholder {
	border: 5px solid #CCC;
	background: #EEE;
	color: #666;
	margin-bottom: 5px;
	margin-left: auto; 
	margin-right: auto;
}
div.family_photo {
	height: 125px; width: 150px; 
}
.family_button {
	margin-right: 17px;
}
div#signup {
	margin-bottom: 12px;
	text-align: left;
}
/***** Product Family Page *****/
.small_product_box,
.small_product_boxistomp,
.small_product_boxepedal,
.small_product_boxhardwire {
	background: white;
	-webkit-border-radius: 6px;
	-moz-border-radius: 6px;
	border-radius: 6px;
	border: 1px solid #CCC;
	padding: 6px;
	text-align: center;	
	width: 130px;
	height: 210px;
	float: left;
	margin: 6px;
	position: relative;
	.compare_checkbox {
		position: absolute;
		top: 80px;
		left: 0px;
		text-align: center;
		width: 140px;
	}
}
.small_product_boxvocalist {
	background: white url(<%= image_path('vocalist/title_bg.png') %>) repeat-x;
	-moz-border-radius: 4px;
	border-radius: 4px;
	border: 1px solid #5b252a;
	padding: 6px;
	text-align: center;	
	width: 130px;
	height: 210px;
	float: left;
	margin: 6px;
	position: relative;
	box-shadow: #DBD4C7 1px 2px 5px;
	position: relative;
	.compare_checkbox {
		position: absolute;
		top: 80px;
		left: 0px;
		text-align: center;
		width: 140px;
	}
	h3 {
		position: absolute;
		top: 10px;
		width: 138px;
		left: 2px;
		text-align: center;
		a:link, a:visited {
			color: white;
			text-decoration: none;
		}
	}
	table.product_thumb {
		height: 106px;
		width: 100%;
		margin-top: 25px;
		td {
			text-align: center;
			width: 100%;
		}
	}
}
table.product_thumb {
	height: 106px;
	width: 100%;
	td {
		text-align: center;
		width: 100%;		
	}
}
.related_products {
	margin: 20px 0px;
	font-style: oblique;
}
table.comparison {
	border-collapse: collapse;
	border: 1px solid #CCC;
	td {
		padding: 5px;
		border: 1px dotted #CCC;
		&.spec_name {
			width: 120px;
			background: #EEE
		}
	}
	th {
		padding: 4px;
		border: 1px solid #CCC;
		border-bottom: 2px solid #999;
	}
}
div.compare_button {
	padding: 20px 10px;
}
/***** Product Page *****/
div#product_content {
	margin-bottom: 10px;
	#right_column {
		float: right;
		text-align: left;
		width: 375px;
		h1 {
			font: {
				family: 'Scriber';
				size: 40px;
			}
			text-align: right;
			color: #EEE;
			background: #040437;
			padding: 8px 6px 2px 6px;
			margin-bottom: 2px;
		}
	}
	#left_column {
		float: left;
		text-align: center;
		width: 556px;
	}
}
ul#product_thumbnails {
	margin: 8px 0px;
	padding: 4px;
	text-align: left;
	background: white;
	border: 1px solid #CCC;
	width: 542px;
	li {
		display: inline;
		list-style: none;
		padding: 6px;
	}
}
ul#discontinued_product_thumbnails {
	margin: 14px 0px;
	padding: 4px;
	text-align: left;
	border: none;
	li {
		display: inline;
		list-style: none;
		padding: 6px;
	}
}
#viewer_container {
	height: 366px;
	width: 550px;
	border: 1px solid #CCC;
	background: white;
}
div#product_bottom_content {
	div#left_column {
		margin-right: 250px;
		min-height: 450px;
	}
	div#right_column {
		float: right;
		width: 232px;
		div.product_tab_content {
			width: 200px;
		}
	}
}
div.product_main_tab_content {
	padding-right: 20px;
}
ul#product_main_tabs {
	margin: 0px -10px 8px -10px;
	padding: 0;
	padding-left: 2px;
	border-bottom: 1px solid #E6E6E6;
	height: 47px;
	/*background: url(<%= image_path('lexicon/light_gray_gradient_reversed.png') %>) repeat-x bottom;*/
	li {
		float: left;
		margin: 21px 6px 0px;
		a:link, a:visited {
			display: block;
			padding: 4px 10px;
			border: 1px solid #E6E6E6;
			border-bottom: none;
			color: #666;
			text-decoration: none;
			text-transform: uppercase;
			font-family: 'Scriber';
			background: white;
		}
		a:hover {
			color: #111;
		}
		&.current {
			margin-top: 17px;
			a {
				padding-bottom: 7px;
				padding-top: 6px;
				border-bottom: none;
				color: #111;
			}
		}
	}
}

div#features_content {
	ul {
		margin-left: 20px;
		li {
			padding-left: 6px;
			list-style-type: disc;
		}
	}
}
#description {
	text-align: left;
	margin-top: 10px;
	margin-bottom: 10px;
	ul {
		margin-left: 20px;
		li {
			list-style-type: normal;
			list-style: disc;
			margin-left: 10px;
			padding-left: 4px;
		}
	}
}
div#sub_products {}
div.sub_product {
	float: left;
	text-align: center;
	width: 140px;
	height: 180px;
	margin: 6px auto;
	overflow: hidden;
	position: relative;
}

#breadcrumbs {
	font-size: 90%;
	text-align: left;
	margin: 0px;
	margin-bottom: 4px;
	letter-spacing: 0.02em;
}
.product_detail_box {
	margin-bottom: 10px;
	h2 {
		margin: 0; 
		padding: 0;
		font-size: 16px;
		a:link, a:visited {
			color: #3d73aa; /*#040437;*/
			background: #f1fbfe url(<%= image_path('digitech/tabbg.png') %>);
			display: block;
			-webkit-border-radius: 8px;
			-moz-border-radius: 8px;
			border-radius: 8px;
			border: 1px solid #b5d4f3;
			padding-left: 12px;
			padding-top: 2px;
		}
		a:hover {
			background: #3d73aa; /*#040437;*/
			color: white;			
		}
	}
}

#product_packaging_tabs {
	ul {
		margin:0; 
		padding:0;
		margin-bottom: -8px;
		li {
			margin:0; padding:0;
			display: inline;
			list-style: none;
			border: none;
			a {
				display: block;
				float: left;
				height: 30px;
				padding: 3px 4px 2px 2px;
				margin-right: 4px;
				text-decoration: none;
				border: 1px solid #CCC;
				background: #EEE;
				margin-bottom: 4px;
				color: #040437;
				font-size: 90%;
				letter-spacing: -1px;
				&:hover {
					background: #FFF;
					border: 1px solid #b5d4f3;
				}
			}
			&.current {
				a {
					background: #f1fbfe url(<%= image_path('digitech/tabbg.png') %>);
					border: 1px solid #b5d4f3;
					border-bottom: none;
					padding-bottom: 8px;
					margin-bottom: -4px;
					z-index: 100000;					
				}
			}
		}
	}
}

#product_packaging_contents {
	clear: both;
	border: 1px solid #b5d4f3;
	margin-bottom: 10px;
	padding: 16px 21px;
	height: 230px;
	overflow: auto;
	background: url(<%= image_path('digitech/tabbg_flipped.png') %>) repeat-x;
	ul {
		li {
			list-style-type: disc;
			margin-left: 14px;
		}
	}
}
h2.special_offer {
	text-align: center;
	a:link, a:visited {
		display: block;
		background: #009933;
		color: white;
		-webkit-border-radius: 8px;
		-moz-border-radius: 8px;
		border-radius: 8px;
	}
	a:hover {
		background: #33ff33;
	}
}
.special_offer_contents {
	margin: 0px 3px 12px;
}
.product_tab_content {
	border: 1px solid #b5d4f3;
	padding: 10px;
	width: 330px;
	margin-left: auto;
	margin-right: auto;
	margin-top: -1px;
	max-height: 230px;
	overflow: auto;
}
table.specs {
	border-collapse: collapse;
	margin-top: 15px;
	margin-bottom: 10px;
}
table.specs td {
	padding: 4px 10px;
	border-bottom: 1px dotted #555;
	&.name {
		background: #222;
		color: white;
	}
}
.odd {}
.even {
	background: #EEE;
}
.faq {
	margin-bottom: 8px;
	h3 {
		font-size: 14px;
	}
}
span.discontinued {
	color: red;
}
div#product_buy_now_box {
	text-align: center;
	padding: 10px;
	margin: 10px 0px 20px 0px;
}
#blanket {
	background-color:#111;
	opacity: 0.65;
	filter:alpha(opacity=65);
	position:absolute;
	z-index: 9001;
	top:0px;
	left:0px;
	width:100%;
}
#dealer_popup {
	position:absolute;
	background-color:#000;
	width:720px;
	height:380px;
	z-index: 9002;
	border: 6px solid white;
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px;
	padding: 12px;
	opacity: 0.9;
	filter:alpha(opacity=90);
	#dealers {
		height: 310px;
		overflow: auto;
		margin-bottom: 5px;
	}
	a:link, a:visited {
		color:white;
	}
}
#dealer_popup_close {
	position:absolute;
	z-index: 9004;
	opacity: 0.9;
	filter:alpha(opacity=90);
	#close {
		a:link, a:visited {
			color: #111;
			text-decoration: none;
			font-weight: bold;
		}
	}
}

/***** Dealer Lookup *****/
div#dealer_lookup_floater {
	float: right;
	text-align: right;
	width: 650px;
}
.form_container {
	background: #556A8F;
	color: #FFF;
	padding: 8px 12px 2px 18px;
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px;
	margin-bottom: 6px;
	a:link, a:visited {
		color: white;
	}
}
ul.dealer_results li {
	margin-bottom: 12px;
}
div#pop_info {
	width: 275px;
	text-align: center;
	background: #EEE;
	padding: 8px;
	float: left;
	margin: 15px;
	margin-right: 5px;
	p {
		text-align: left;
		margin: 6px;
	}
}
.marker_info {
	font-size: 11px;
	color: black;
}
div#wtb_container {
	width: 860px;
	margin: 20px auto;
}
div#dealer_results {
	width: 420px;
	float: right;
	padding: 14px;
	padding-top: 0px;
	margin-top: 0px;
	max-height: 575px;
	overflow: auto;
	h2 {
		padding: 0px;
		margin: 0px;
		margin-bottom: 2px;
	}
}
ul.dealer_results {
	li {
		padding: 3px;
		&.current {
			background: #E1F3FF;
		}
	}
}
div.retailer_logo {
  float: left;
  margin: 6px;
  width: 125px;
}
/****** Artist Pages ******/
div.artist_listing {
	width: 262px;
	margin: 10px;
	float: left;
	padding: 0px 10px;
	height: 200px;
	overflow: hidden;
	div.artist_photo {
		float: left;
		margin: -2px 8px 1px 0px;
	}
	h2 {
		font-size: 20px;
		line-height: 21px;
	}
}
div.on_the_left {
	margin-left: 0px;
	margin-right: 0px;
}

div#featured_artist_box {
    margin-left: -10px;
	text-align: center;
	width: 940px;
	height: 400px; 
	outline: 1px solid #2c3868;
	position: relative;
	margin-bottom: 20px;
	.slideshow_frame {
		position: relative;	
		top: 0px;
		width: 940px;
		height: 400px;
		.description {
			text-align: left;
			padding: 4px 200px 10px 40px;
			position: absolute;
			bottom: 0px;
			left: 0px;
			width: 700px;
			background: black;
			z-index: 1000;
			color: white;
			zoom: 1;
    		filter: alpha(opacity=70);
    		opacity: 0.7;
		}
	}
	div#slideshow_controls {
		bottom: 8px;
		right: 4px;
	}
}
div#big_artist_photo {
	text-align: center;
	margin-bottom: 10px;
	margin-left: -10px;
	width: 940px; 
	height: 400px; 
	outline: 1px solid #2c3868;
}
div#artist_product_photo_medium {
	text-align: center;
	width: 250px;
	height: 250px;
	outline: 1px solid #2c3868;
	margin-bottom: 8px;
}
div#artist_callout {
	float: left;
