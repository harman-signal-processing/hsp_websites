body {
	font-family: Arial, Verdana, Sans-serif;
	font-size: 12px;
	line-height: 1.4;
	color: #FFF;
	background: #2a3333 url(<%= image_path('hardwire/page_bg.png') %>) no-repeat top center;
	margin: 0;
}
@font-face {
    font-family: 'BebasRegular';
    src: url('/fonts/BebasNeue-webfont.eot');
    src: url('/fonts/BebasNeue-webfont.eot?#iefix') format('embedded-opentype'),
         url('/fonts/BebasNeue-webfont.woff') format('woff'),
         url('/fonts/BebasNeue-webfont.ttf') format('truetype'),
         url('/fonts/BebasNeue-webfont.svg#BebasNeueRegular') format('svg');
    font-weight: normal;
    font-style: normal;

}
p {
	margin-bottom: 8px;
}
a:link, a:visited {
	color: blue;
}
li {
	margin-bottom: 6px;
}
h1, h2 {
	margin-bottom: 8px;
	padding: 0px;
	color: #EEE;
	font-weight: 500;
	font-family: 'BebasRegular', sans-serif;
	font-size: 110%;
	letter-spacing: 0.1em;
}
h1 {
	font-size: 34px;
	line-height: 40px;
	&.page_title {
		text-align: center;
	}
}
h2 {
	font-size: 22px;
	line-height: 36px;
	a:link, a:visited {
		text-decoration: none;
	}
}
h3 {
	font-size: 18px;
	line-height: 20px;
	margin-bottom: 4px;
}
h4 {
	font-size: 16px;
}
h5 {
	font-size: 14px;
}
h6 {
	font-size: 12px;
}
.tiny {
	font-size: 10px;
}
.clear {
	clear: both;
}
#preload {
	display: none;
}
/**** Application Layout *****/
header#main_header {
	height: 50px;
	overflow: hidden;
	width: 960px;
	margin: -1px auto 0px;
	padding-top: 16px;
	position: relative;
	z-index: 5000;
}
div#wide_line {
	position: absolute;
	top: 84px;
	left: 0px;
	width: 100%;
	border-bottom: 1px solid #384343;
}
nav#topnav {
	float: right;
	height: 23px;
	width: 770px;
	margin-top: 15px;
	padding-top: 6px;
	padding-left: 2px;
	font-family: 'BebasRegular', Arial, Helvetica, sans-serif;
	font-size: 14px;
/*	font-weight: bold;*/
	text-transform: uppercase;
	/*letter-spacing: -0.09em;*/
	-moz-border-radius: 8px;
	border-radius: 8px;
	text-align: right;
	letter-spacing: 0.08em;
	font-weight: 200;
	background: none;
	ul {
		li {
			display: inline;
			list-style: none;
			a:link, a:visited {
				text-shadow: #000 0px 1px 1px;
				padding: 0px 8px 1px 11px;
				color: #CCC;
				text-decoration: none;
				margin-top: 2px;
				background: url(<%= image_path('digitech/hw_right_arrow.png') %>) no-repeat;
				background-position: 0px 2px;
				&.nodivider {
					border: none;
				}
			}
			a:hover {
				color: white;
				text-decoration: underline;
			}
		}
	}
}
div#search_form {
	display: none;
}
div#page_container {
	width: 960px;
	margin-left: auto;
	margin-right: auto;
	border: 1px solid #384343;
	border-top: none;
    -moz-box-shadow: 0px 4px 15px #000;
	-webkit-box-shadow: 0px 4px 15px #000;
	box-shadow: 0px 4px 15px #000;
}
div#content_container {
	width: 960px;
	margin-left: auto;
	margin-right: auto;
}
div#content_container_content {
	padding: 10px;
}
.internal_page {
	color: #333;
	min-height: 350px;
	background: #FFF;
	h1, h2 {
		color: #111;
	}
}
div#logo {
	float: left;
	margin-top: 10px;
	margin-left: 0px;
	h2 {
		font-size: 30px;
		color: white;
		margin-top: -5px;
		text-align: center;
		width: 170px;
		a:link, a:visited, a:hover {
			color: white;
			text-decoration: none;
			text-shadow: #000 1px 1px 1px;
		}
	}
}
div#big_bottom_box_container {
	color: white;
	margin: 0px auto;
	background: #2a3333;
	border-top: 1px solid #384343;
	border-bottom: 1px solid #384343;
	padding-bottom: 10px;
}
div#big_bottom_box_content {
	padding: 10px;
	padding-bottom: 20px;
	a:link,	a:visited {
		color: #ccc;
	}
	h2 {
		font-size: 18px;
		a:link, h2 a:visited {
			color: #ccc;
		}
	}
}
.divider_above {
	border-top: 1px dotted #7283AF; 
	margin-top: 20px;
}
footer {
	width: 940px;
	margin: 10px auto;
	font-size: 10px;
	font-family: Arial, Helvetica, Sans-serif;
	color: #fff;
	text-align: center;
	height: 60px;
	a:link, a:visited {
		color: white
	}
	nav#footer_info {
		float: right;
		text-align: right;
		margin-top: 10px;
	}
	div#footer_left {
		float: left;
		img {
			margin-bottom: -3px;
			margin-right: 2px;
		}
	}
}

#social_media_links {
	text-align: center;
	margin-bottom: 10px;
	img {
		margin-left: 8px;
		margin-right: 8px;
	}
}
.medium_centered_box {
	width: 500px;
	margin-left: auto;
	margin-right: auto;
	margin-top: 20px;
	margin-bottom: 30px;
	padding: 20px;
}
.wide_centered_box {
	width: 790px;
	margin-left: auto;
	margin-right: auto;
	margin-top: 4px;
	margin-bottom: 10px;
}
.article_centered_box {
	margin: 10px 10%;
	h1 {
		font-size: 24px;
		line-height: 27px;
	}
	.little {
		font-style: oblique;
		color: #555;
	}
}
.light_gray {
	background: #2a3333;
	color: #EEE;
}
/***** columns ******/
div.two_column_container {
	text-align: center;
	.column_one {
		float: left;
		padding: 10px;
		width: 45%;
		text-align: left;
	}
	.column_two {
		float: right;
		padding: 10px;
		width: 45%;
		text-align: left;
	}
}
/**** Main ******/
div#middle_container {
	text-align: center;
	width: 962px;
	margin-left: auto;
	margin-right: auto;
}
div.product_family_feature {
	background: #2a3333;
	border: 1px solid #384343;
/*	margin: 20px;*/
	padding: 10px;
	width: 218px;
	height: 50px;
	float: left;
	padding-top: 30px;
	text-align: right;
	color: #FFF;
	font-family: 'BebasRegular', Arial, sans-serif;
	h2 {
		letter-spacing: 0.12em;	
		line-height: 16px;
		margin-bottom: 1px;
		a:link, a:visited {
			color: #a1b020;
			font-size: 80%;
			background: none;
			padding: 0;
			text-transform: none;
		}
	}
	a:link, a:visited {
		color: white;
		font-size: 115%;
		text-decoration: none;
		padding-left: 13px;
		background: url(<%= image_path('digitech/hw_right_arrow.png') %>) no-repeat;
		background-position: 0px 2px;
		letter-spacing: 0.06em;
		text-transform: uppercase;
	}
}
.homepage_feature {
	margin-right: 4px;
}
div.placeholder {
	border: 5px solid #CCC;
	background: #EEE;
	color: #666;
	margin-bottom: 5px;
	margin-left: auto; 
	margin-right: auto;
}
div.family_photo {
	height: 125px; width: 150px; 
}
div#signup {
	margin-bottom: 12px;
	text-align: left;
}
/***** Product Family Page *****/
.small_product_box {
	background: white;
	color: #333;
	-moz-border-radius: 6px;
	border-radius: 6px;
	border-radius: 6px;
	border: 1px solid #CCC;
	padding: 6px;
	text-align: center;	
	width: 130px;
	height: 180px;
	float: left;
	margin: 6px;
	position: relative;
	.compare_checkbox {
		position: absolute;
		top: 80px;
		left: 0px;
		text-align: center;
		width: 140px;
	}
}
.small_product_boxhardwire {
	background: white;
	padding: 6px;
	text-align: center;	
	width: 130px;
	height: 210px;
	float: left;
	margin: 6px;
	position: relative;
	.compare_checkbox {
		position: absolute;
		top: 80px;
		left: 0px;
		text-align: center;
		width: 140px;
	}
}
table.product_thumb {
	height: 106px;
	width: 100%;
	td {
		text-align: center;
		width: 100%;
	}
}
.related_products {
	margin: 20px 0px;
	font-style: oblique;
}
table.comparison {
	border-collapse: collapse;
	border: 1px solid #CCC;
	td {
		padding: 5px;
		border: 1px dotted #CCC;
		&.spec_name {
			width: 120px;
			background: #EEE
		}
	}
	th {
		padding: 4px;
		border: 1px solid #CCC;
		border-bottom: 2px solid #999;
	}
}
div.compare_button {
	padding: 20px 10px;
}
/***** Product Page *****/
div#product_content {
	margin-bottom: 10px;
	#right_column {
		float: right;
		text-align: left;
		width: 375px;
		h1 {
			font-family: 'BebasRegular', sans-serif;
			letter-spacing: 0.05em;
			text-align: right;
			font-size: 40px;
			color: #EEE;
			background: #a1b020;
			padding: 8px 6px 2px 6px;
			margin-bottom: 2px;
			text-shadow: #333 0px 1px 1px;
		}
	}
	#left_column {
		float: left;
		text-align: center;
		width: 540px;
		font-size: 14px;
	}
}
ul#product_thumbnails {
	margin: 8px 0px;
	padding: 4px;
	text-align: left;
	background: white;
	border: 1px solid #CCC;
	width: 542px;
	li {
		display: inline;
		list-style: none;
		padding: 6px;
	}
}
ul#discontinued_product_thumbnails {
	margin: 14px 0px;
	padding: 4px;
	text-align: left;
	border: none;
	li {
		display: inline;
		list-style: none;
		padding: 6px;
	}
}
#viewer_container {
	height: 366px;
	width: 550px;
	border: 1px solid #CCC;
	background: white;
}
#description {
	text-align: left;
	margin-top: 10px;
	margin-bottom: 10px;
}
#breadcrumbs {
	font-size: 90%;
	text-align: left;
	margin: 0px;
	margin-bottom: 4px;
	letter-spacing: 0.02em;
}
.product_detail_box {
	margin-bottom: 10px;
	h2 {
		margin: 0; padding: 0;
		font-size: 16px;
		a:link, a:visited {
			color: #CCC;
			border: 1px solid #444;
			background: #191F1F url(<%= image_path('digitech/hw_right_arrow.png') %>) no-repeat;
			background-position: 9px 13px;
			display: block;
			padding-left: 22px;
			padding-top: 2px;
		}
		a:hover {
			color: white;
			background-color: #2a3333;
		}
	}
}
#product_packaging_tabs {
	ul {
		margin:0; padding:0;
		margin-bottom: -8px;
		li {
			margin:0; padding:0;
			display: inline;
			list-style: none;
			border: none;
			a {
				display: block;
				float: left;
				height: 30px;
				padding: 3px 4px 2px 2px;
				margin-right: 4px;
				text-decoration: none;
				border: 1px solid #CCC;
				background: #EEE;
				margin-bottom: 4px;
				color: #040437;
				font-size: 90%;
				letter-spacing: -1px;
				&:hover {
					background: #FFF;
					border: 1px solid #b5d4f3;
				}
			}
			.current a {
				background: #f1fbfe url(<%= image_path('tabbg.png') %>);
				border: 1px solid #b5d4f3;
				border-bottom: none;
				padding-bottom: 8px;
				margin-bottom: -4px;
				z-index: 100000;
			}
		}
	}
}
#product_packaging_contents {
	clear: both;
	border: 1px solid #444;
	background: #EEE;
	margin-bottom: 10px;
	padding: 16px 21px;
	height: 230px;
	overflow: auto;
}
h2.special_offer {
	text-align: center;
	a:link,	a:visited {
		display: block;
		background: #009933;
		color: white;
		-moz-border-radius: 8px;
		border-radius: 8px;
	}
	a:hover {
		background: #33ff33;
	}
}
.special_offer_contents {
	margin: 0px 3px 12px;
}
.product_tab_content {
	border: 1px solid #888;
	padding: 10px;
	width: 354px;
	margin-left: auto;
	margin-right: auto;
	margin-top: -1px;
	max-height: 230px;
	min-height: 110px;
	overflow: auto;
	background: #EEE;
}
table.specs {
	border-collapse: collapse;
	margin-top: 15px;
	margin-bottom: 10px;
	td {
		padding: 4px 10px;
		border-bottom: 1px dotted #555;
		.name {
			background: #222;
			color: white;
		/*	border-bottom: 1px solid #CCC;*/
		}
	}
}
.odd {}
.even {
	background: #EEE;
}
.faq {
	margin-bottom: 8px;
}
.faq h3 {
	font-size: 14px;
}
span.discontinued {
	color: red;
}
div#product_buy_now_box {
	text-align: center;
	padding: 10px;
	margin: 10px 0px 10px 0px;
}
#blanket {
	background-color:#111;
	opacity: 0.65;
	filter:alpha(opacity=65);
	position:absolute;
	z-index: 9001;
	top:0px;
	left:0px;
	width:100%;
}
#dealer_popup {
	position:absolute;
	background-color:#000;
	width:720px;
	height:380px;
	z-index: 9002;
	border: 6px solid white;
	-moz-border-radius: 8px;
	border-radius: 8px;
	padding: 12px;
	opacity: 0.9;
	filter:alpha(opacity=90);
}
#dealer_popup #dealers {
	height: 310px;
	overflow: auto;
	margin-bottom: 5px;
}
#dealer_popup a:link,
#dealer_popup a:visited {
	color:white;
/*	display: block;*/
}
#dealer_popup_close {
	position:absolute;
	z-index: 9004;
	opacity: 0.9;
	filter:alpha(opacity=90);
}
#dealer_popup #close a:link,
#dealer_popup #close a:visited {
	color: #111;
	text-decoration: none;
	font-weight: bold;
}
/***** Dealer Lookup *****/
