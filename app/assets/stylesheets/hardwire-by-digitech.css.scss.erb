	padding-top: 0px;
	margin-top: 0px;
	max-height: 600px;
	overflow: auto;
}
div#dealer_results h2 {
	padding: 0px;
	margin: 0px;
	margin-bottom: 2px;
}
ul.dealer_results li {
	padding: 3px;
}
ul.dealer_results li.current {
	background: #3B4A4A;
}
div.retailer_logo {
  float: left;
  margin: 6px;
  width: 125px;
}
