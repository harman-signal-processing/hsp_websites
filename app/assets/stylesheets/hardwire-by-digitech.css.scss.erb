	bottom: 8px;
	right: 4px;
}
div.artist_listing div.artist_photo {
	float: left;
	margin: -2px 8px 1px 0px;
}
div.artist_listing h2 {
	font-size: 20px;
	line-height: 21px;
}
div#big_artist_photo {
	text-align: center;
	margin-bottom: 10px;
	margin-left: -10px;
	width: 940px; 
	height: 400px; 
	outline: 1px solid #384343;
}
div#artist_product_photo_medium {
