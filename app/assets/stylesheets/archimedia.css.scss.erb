@font-face {
    font-family: 'AsapRegular';
    src: url('/fonts/Asap-Regular-webfont.eot');
    src: url('/fonts/Asap-Regular-webfont.eot?#iefix') format('embedded-opentype'),
         url('/fonts/Asap-Regular-webfont.woff') format('woff'),
         url('/fonts/Asap-Regular-webfont.ttf') format('truetype'),
         url('/fonts/Asap-Regular-webfont.svg#AsapRegular') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'AsapItalic';
    src: url('/fonts/Asap-Italic-webfont.eot');
    src: url('/fonts/Asap-Italic-webfont.eot?#iefix') format('embedded-opentype'),
         url('/fonts/Asap-Italic-webfont.woff') format('woff'),
         url('/fonts/Asap-Italic-webfont.ttf') format('truetype'),
         url('/fonts/Asap-Italic-webfont.svg#AsapItalic') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'AsapBold';
    src: url('/fonts/Asap-Bold-webfont.eot');
    src: url('/fonts/Asap-Bold-webfont.eot?#iefix') format('embedded-opentype'),
         url('/fonts/Asap-Bold-webfont.woff') format('woff'),
         url('/fonts/Asap-Bold-webfont.ttf') format('truetype'),
         url('/fonts/Asap-Bold-webfont.svg#AsapBold') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'AsapBoldItalic';
    src: url('/fonts/Asap-BoldItalic-webfont.eot');
    src: url('/fonts/Asap-BoldItalic-webfont.eot?#iefix') format('embedded-opentype'),
         url('/fonts/Asap-BoldItalic-webfont.woff') format('woff'),
         url('/fonts/Asap-BoldItalic-webfont.ttf') format('truetype'),
         url('/fonts/Asap-BoldItalic-webfont.svg#AsapBoldItalic') format('svg');
    font-weight: normal;
    font-style: normal;
}
body {
	font-family: 'AsapRegular', Helvetica, Arial, Verdana, Sans-serif;
	font-size: 12px;
	line-height: 1.8;
	color: #464646;
	background: #FFF;
	margin: 0;
}
p {
	margin-bottom: 8px;
}
a {
	&:link, &:visited {
		color: #83b341;
		&.button {
			border-radius: 8px;
			-moz-border-radius: 8px;
		/*	border: 1px solid #CECECE;*/
			padding: 4px 10px;
			font-size: 18px;
			text-shadow: -1px -1px 0px #A9A9A9;
			color: #555;
			font-weight: bold;
			font-family: 'AsapRegular', Arial, Helvetica, sans-serif;
			text-decoration: none;
			background: transparent url(<%= image_path('archimedia/browse_products_background.png') %>) repeat-x;
			background-position: 0px 95%;
			box-shadow: 0px 2px 2px #6E6E6E;
		}
	}
	&:hover.button {
		color: #222;
		background-position: 0px 10%;
	}
}
li {
	margin-bottom: 6px;
}
h1, h2 {
	margin-bottom: 8px;
	padding: 0px;
	color: #969696;
	font-weight: 500;
	font-family: 'AsapRegular', sans-serif;
	font-size: 110%;
	letter-spacing: 0.04em;
}
h1 {
	font-size: 34px;
	line-height: 40px;
	.page_title {
		text-align: center;
	}
}
h2 {
	font-size: 22px;
	line-height: 36px;
	font-family: 'AsapRegular', sans-serif;
	text-transform: uppercase;
	a:link, a:visited {
		text-decoration: none;
	}
}

h3{
font-weight: 500;
	font-size: 16px;
	line-height: 20px;
	margin-bottom: 4px;
	font-family: 'AsapRegular', sans-serif;
	a:link, a:visited {
		text-decoration: none;
	}
}
h4{
	font-size: 16px;
}
h5{
	font-size: 14px;
}
h6{
	font-size: 12px;
}
.tiny {
	font-size: 10px;
}
.clear {
	clear: both;
}
.strike {
	text-decoration: line-through;
}
#preload {
	display: none;
}
.narrow_content {
  margin: 40px 20%;
}
/**** Application Layout *****/
header {
	height: 130px;
	position: fixed;
	top: 0px;
	overflow: hidden;
	width: 100%;
	margin: -1px auto 0px;
	padding-top: 16px;
	z-index: 89;
	background: url(<%= image_path('archimedia/header_background.png') %>) repeat-x top;
	behavior: url("/css/iepngfix.htc");

}
div#inner_header {
	position: relative;
	width: 930px;
	margin: 0px auto;
}
div#supernav {
	line-height: 1.4;
	position: absolute;
	top: 12px;
	right: -7px;
	text-align: right;
    padding: 0px;
	height: 44px;
	div#browse {
		position: absolute;
		top: 18px;
		left: -410px;
		width: 400px;
		color: #CCC;
		letter-spacing: 0.18em;
		text: {
			align: right;
			transform: uppercase;
			shadow: 1px 1px 0px #464646;
		}
		font: {
			family: 'AsapRegular', Arial, Helvetica, sans-serif;
			size: 14px;
			weight: 500;
		}
		img {
			vertical-align: middle;
		}
	}
	ul {
		padding-top: 11px;
		height: 32px;
		border-radius: 8px;
		-moz-border-radius: 8px;
		box-shadow: 0px 2px 6px #282828;
	}
	li {
		display: inline-block;
		background: transparent url(<%= image_path('archimedia/divider.png') %>) repeat-y;
		background-position: right;
		list-style: none;
		padding: 11px 0px 13px 0px;
		margin: -11px 0 -13px;
		float: left;
		position: relative;
		a:link, a:visited {
			letter-spacing: 0.11em;
			margin: 0 2px 0 0;
			padding: 12px 20px 15px 20px;
			color: #6d6d6d;
			text: {
				decoration: none;
				transform: uppercase;
				shadow: #8B8B8B 0px 1px 1px;
			}
			font: {
				family: 'AsapRegular', Arial, Helvetica, sans-serif;
				size: 13px;
			}
			background: transparent url(<%= image_path('archimedia/browse_products_background.png') %>) repeat-x bottom;
			&.nodivider {
				border: none;
			}
		}
		a:hover {
			background-position: top;
		}
		&:first-child {
			border-left: none;
			a:link {
				border-top-left-radius: 8px;
				border-bottom-left-radius: 8px;
				-moz-border-radius-topleft: 8px;
				-moz-border-radius-bottomleft: 8px;
				padding-left: 25px;
			}
		}
		&:last-child, &.last-child {
			border-right: none;
			background: none;
			margin-right: -1px;
			a:link {
				border-top-right-radius: 8px;
				border-bottom-right-radius: 8px;
				-moz-border-radius-topright: 8px;
				-moz-border-radius-bottomright: 8px;
				padding-right: 25px;
			}
		}
	}
}

div#topnav {
	position: absolute;
	top: 85px;
	left: -3px;
	z-index: -10;
	height: 28px;
	width: 930px;
	padding: 7px 6px 3px 0px;
	font-family: 'AsapBold', Arial, Helvetica, sans-serif;
	font-size: 14px;
	text-transform: uppercase;
	-moz-border-radius-bottomleft: 6px;
	-moz-border-radius-bottomright: 6px;
	border-bottom-left-radius: 6px;
	border-bottom-right-radius: 6px;
	font-weight: normal;
	background: transparent url(<%= image_path('archimedia/nav_background.png') %>) repeat-x top;
	box-shadow: 0px 1px 5px #444;
	ul {
		padding-top: 0px;
	}
	li {
		display: inline;
		list-style: none;
		margin: 0;
		padding: 0;
		a:link, a:visited {
			font-family: 'AsapBold', Arial, Helvetica, sans-serif;
			letter-spacing: 0.05em;
			text: {

				decoration: none;
			}
			padding: 0px 6px 1px 11px;
			color: #999;
			margin-top: 2px;
			&.nodivider {
				border: none;
			}
		}
		a:hover {
			color: #777;
		}
	}
}

div#supersocial {
	position: absolute;
	top: 56px;
	right: -4px;
	z-index: 100;
	a {
		padding-left: 12px;
	}
	img {
	    filter: url(/filters.svg#desaturate); /* Firefox 3.5+ */
	    filter: gray; /* IE5+ */
	    -webkit-filter: grayscale(1); /* Webkit Nightlies & Chrome 18+ (currently Beta) */
		-ms-filter: "progid:DXImageTransform.Microsoft.BasicImage(grayscale=1)";
		&:hover {
		    filter: none;
		    -webkit-filter: grayscale(0);
			-ms-filter: none;
		}
	}
}

