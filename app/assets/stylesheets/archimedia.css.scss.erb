/***** Product Page *****/
div#product_content {
	margin-bottom: 10px;
	.suggested_product a {
		text-decoration: none;
		color: #555;
		h3 {
			color: #000;
		}
	}
	#right_column {
		float: right;
		text-align: left;
		width: 222px;
		padding: 3px 6px;
		border: 1px solid #E6E6E6;
		height: 470px;
		overflow: hidden;
		background: #f0f0ef url(<%= image_path('archimedia/light_gray_gradient.png') %>) repeat-x;
		h4 {
			text-align: center;
			font-size: 10px;
			line-height: 14px;
			font-weight: 100;
		}
		.suggested_product {
			margin: 20px 6px;
			padding-bottom: 8px;
			border-bottom: 1px solid #CCC;
		}
	}
	#left_column {
		float: left;
		text-align: center;
		width: 700px;
		position: relative;
		#product_basics {
			font-family: 'AsapRegular', sans-serif;
			color: #797979;
			font-size: 14px;
			line-height: 20px;
			h1 {
				font-family: 'AsapBold', sans-serif;
				font-size: 30px;
				line-height: 40px;
				color: #1A1A1A;
				text-shadow: #A5A5A5 0px 1px 1px;
			}
			h3 {
				font-family: 'AsapRegular', sans-serif;
				color: #797979;
				margin-bottom: 16px;
			}
		}
		h2.price {
			font-family: 'AsapBold', sans-serif;
			font-size: 20px;
			line-height: 26px;
			color: #AE9A6A;
		}
	}
}
div#product_content.vertical {
	#left_column {
		#product_basics {
			position: absolute;
			top: 40px;
			right: 26px;
			width: 260px;
			z-index: 70;
			text-align: left;
		}
	}
	#viewer_container {
		height: 366px;
		width: 400px;
		background: white;
		text-align: center;
		display: table;
		margin-bottom: 40px;
	}
	#viewer {
		max-width: 400px;
		overflow: hidden;
	}
}

div#product_content.horizontal {
	#left_column {
		#product_basics {
			position: absolute;
			top: -16px;
			left: 16px;
			z-index: 70;
			text-align: left;
			#product_basics_bottom {
				margin-top: 266px;
			}
		}
	}
	#viewer_container {
		height: 275px;
		width: 700px;
		margin-top: 65px;
		margin-bottom: 65px;
		background: white;
		text-align: center;
		overflow: hidden;
	}
	#viewer {
		max-width: 700px;
		max-height: 275px;
		overflow: hidden;
	}
}

