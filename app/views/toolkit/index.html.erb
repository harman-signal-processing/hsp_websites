<div style="text-align: center">
	<h4 class="subheader">Welcome to the</h4>
	<h3>Harman Signal Processing Marketing Toolkit</h3>
	<h6 class="subheader">(Yes, that is a really long name. Feel free to call it the "HSPMT")</h6>
</div>

<div class="row" style="margin: 40px 0px;">
<div class="eight columns offset-by-two" style="text-align: center">
	<% toolkit_brands.each do |brand| %>
		<% if brand.logo.present? %>
				<%= link_to(image_tag(brand.logo.url(:thumb), style: "margin: 10px"), [:toolkit, brand]) %>
		<% end %>
		<% end %>
	<ul>
	<% toolkit_brands.each do |brand| %>
		<% if brand.logo.blank? %>
		<li><%= link_to(image_tag(brand.logo), [:toolkit, brand]) %></li>
		<% end %>
	<% end %>
	</ul>
</div>
</div>

<div class="container" style="text-align: center">

</div>

