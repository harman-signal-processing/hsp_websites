<%= content_for :feature do %>
  <div class="container product_family_header">
    <div class="row">
      <div class="medium-12 small-12 columns">
        <% if @product_family.title.present? %>
          <h1><%= translate_content(@product_family, :title) %></h1>
        <% else %>
          <h1><%= translate_content(@product_family, :name) %></h1>
        <% end %>
        <p><%=raw translate_content(@product_family, :intro) %></p>
      </div> <!-- <div class="large-12 small-12 columns"> -->
    </div> <!-- <div class="row"> -->
  </div> <!-- <div class="container product_family_header">  -->      
<% end %>