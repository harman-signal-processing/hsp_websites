<h1>Artist Tier</h1>

<div class="button_container">
	<%= link_to 'Back', admin_artist_tiers_path, :class => "back button" %>
	<%= link_to 'Edit', edit_admin_artist_tier_path(@artist_tier), :class => "edit button" %>
	<%= link_to 'Delete', [:admin, @artist_tier], :confirm => 'Are you sure?', :method => :delete, :class => "delete button" %>
</div>

<p>
  <b>Name:</b>
  <%= @artist_tier.name %>
</p>

<p>
  <b>Invitation code:</b>
  <%= @artist_tier.invitation_code %>
</p>
