(function(){
	var $list_of_associated_categories = $('#vip_service_service_categories');
	build_list_of_categories_associated_with_this_service($list_of_associated_categories);
	
	function build_list_of_categories_associated_with_this_service($list_of_associated_categories) {
		<% @vip_service_service_categories.each do |service_category| %>
			$list_of_associated_categories.append('<%= escape_javascript(render "/admin/shared/vip_service_service_category_row", association: service_category, called_from: @called_from) %>');
		<% end %>	
	}  //  function build_list_of_categories_associated_with_this_service($list_of_associated_categories)
})();