<%= simple_form_for([:admin, @website_locale]) do |f| %>
  <%= f.association :website, collection: Website.order('url'), label_method: :url %>
  <%= f.input :locale, collection: Locale.all_unique_locales %>
  <%= f.input :complete, label: "Complete, and ready for use" %>
  <br/><%= f.button :submit %>
<% end %>
