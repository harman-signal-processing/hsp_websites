<% called_from ||= "dealer" %>
<li id="brand_dealer_rental_product_<%= item.id %>">
  <span class="handle"><%= image_tag("icons/drag_handle.gif", lazy: false) %></span>
	<%=	link_to(item.product.name, admin_product_path(item.product), target:"_blank")%>
	<%= link_to(image_tag('icons/trash.gif', lazy: false),
		  admin_brand_dealer_rental_product_path(item, called_from: called_from),
		  remote: true,
		  confirm: "Are you sure you want to delete this rental product association?",
		  method: :delete) %>
</li>