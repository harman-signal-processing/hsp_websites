<%= simple_form_for([:admin, @dealer]) do |f| %>

   <%= f.input :name %>
   <%= f.input :address %>
   <%= f.input :city %>
   <%= f.input :state %>
   <%= f.input :zip %>
   <%= f.input :country %>
   <%= f.input :telephone %>
   <%= f.input :fax %>
   <%= f.input :email %>
   <%= f.input :website %>
   <%= f.input :google_map_place_id %>
   <%= f.input :resale %>
   <%= f.input :rental %>
   <%= f.input :installation %>
   <%= f.input :service %>
   <%= f.input :represented_in_country %>
   <%= f.input :account_number %>
   <%= f.input :exclude, label: "Exclude from site listings" %><br/>

  <br/><%= f.button :submit %>

<% end %>
