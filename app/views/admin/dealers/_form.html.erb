<%= simple_form_for([:admin, @dealer]) do |f| %>

   <%= f.input :name %>
   <%= f.input :address %>
   <%= f.input :city %>
   <%= f.input :state %>
   <%= f.input :zip %>
   <%= f.input :telephone %>
   <%= f.input :fax %>
   <%= f.input :email %>
   <%= f.input :account_number %>
   <%= f.input :exclude, label: "Exclude from site listings" %><br/>

  <br/><%= f.button :submit %>

<% end %>