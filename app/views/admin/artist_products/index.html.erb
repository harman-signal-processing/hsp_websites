<h1>Listing artist_products</h1>

<table>
  <tr>
    <th>Artist</th>
    <th>Product</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @artist_products.find_each do |artist_product| %>
  <tr>
    <td><%= artist_product.artist_id %></td>
    <td><%= artist_product.product_id %></td>
    <td><%= link_to 'Show', [:admin, artist_product] %></td>
    <td><%= link_to 'Edit', edit_admin_artist_product_path(artist_product) %></td>
    <td><%= link_to 'Delete', [:admin, artist_product], confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Artist product', new_admin_artist_product_path %>
