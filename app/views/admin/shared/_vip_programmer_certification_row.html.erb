<% called_from ||= 'vip_programmer' %>
<li id="vip_programmer_certification_<%= association.id %>">
	<%= (called_from == 'vip_programmer') ? link_to(association.certification.name, [:admin, association.certification]) : link_to(association.programmer.name, [:admin, association.programmer]) %> 
	<%= link_to(image_tag("icons/trash.gif"), 
			admin_vip_programmer_certification_path(association), 
			confirm: "Are you sure? (This won't delete the certification --it will only remove the association.)", 
			remote: true,
			method: :delete) %>
</li>
