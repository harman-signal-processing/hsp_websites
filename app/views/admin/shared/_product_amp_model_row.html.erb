<% called_from ||= "product" %>
<li id="product_amp_model_<%= pam.id %>">
	<%= (called_from == "product") ?
		link_to(pam.amp_model.name, edit_admin_amp_model_path(pam.amp_model)) :
		link_to(pam.product.name, [:admin, pam.product]) %>
	<%= link_to(image_tag('icons/trash.gif'),
		  admin_product_amp_model_path(pam),
		  remote: true,
		  confirm: "Are you sure you want to delete this amp model?",
		  method: :delete) %>
</li>