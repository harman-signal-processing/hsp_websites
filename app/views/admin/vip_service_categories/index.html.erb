<h1>AMX VIP Service Category</h1>

<ul class="button-group round">
	<li><%= link_to 'Programmers List', admin_vip_programmers_path, class: "small button round" %>	</li>
	<li><%= link_to 'New AMX VIP service_category', new_admin_vip_service_category_path, class: "small success button" %></li>
</ul>

<table class="data">
  <tr>
  	<th></th>
    <th>Name</th>
  </tr>

<% @vip_service_categories.each do |service_category| %>
  <tr style="vertical-align:text-bottom;">
    <td><%= link_to 'Edit', edit_admin_vip_service_category_path(service_category), class: "small round button" %></td>
    <td><%= service_category.name %></td>
  </tr>
<% end %>
</table>