<h1>New product_review_product</h1>

<%= form_for([:admin, @product_review_product]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :product_id %>
    <%= f.text_field :product_id %>
  </p>
  <p>
    <%= f.label :product_review_id %>
    <%= f.text_field :product_review_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', admin_product_review_products_path %>