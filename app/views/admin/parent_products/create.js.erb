(function(){
	<%if @called_from == "product" %>
		<%= render "/admin/parent_products/rebuild_parent_product_list" %>		
	<% end %>

	<% @parent_products.each do |parent_product| %>
		$('#parent_products').append('<%= escape_javascript(render "/admin/shared/parent_product_row", pp: parent_product, called_from: @called_from) %>');
	<% end %>
})();


<% flash.discard %>
