<h1>Listing product_sub_categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Live</th>
      <th>Orderby</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_sub_categories.each do |product_sub_category| %>
      <tr>
        <td><%= product_sub_category.name %></td>
        <td><%= product_sub_category.description %></td>
        <td><%= product_sub_category.live %></td>
        <td><%= product_sub_category.orderby %></td>
        <td><%= link_to 'Show', product_sub_category %></td>
        <td><%= link_to 'Edit', edit_product_sub_category_path(product_sub_category) %></td>
        <td><%= link_to 'Destroy', product_sub_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product sub category', new_product_sub_category_path %>
